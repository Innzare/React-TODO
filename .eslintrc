{
   "parser": "@babel/eslint-parser",
   "parserOptions": {
      "babelOptions": {
         "presets": [
            "@babel/preset-react"
         ]
      }
   },
   "rules": {
      "no-unused-vars": "warn"
   },
   "env": {
      "node": true,
      "commonjs": true,
      "browser": true,
      "es6": true
   },
   "extends": [
      "eslint:recommended"
   ]
}